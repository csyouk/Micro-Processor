#include "device_driver.h"
#define BLOCK 100
typedef unsigned int (* LAYER_FRAME)[LCD_XSIZE];
extern LAYER_FRAME Fb_Layer;
void Save_Img_To_Nand(void){
	unsigned int addr = Nand_Page_2_Addr(BLOCK, 0, 0);
	unsigned short *q = (unsigned short *)CAM_Check_Image_Ready();

	Nand_Write_Block(BLOCK,Fb_Layer);
	Nand_Verify_Block(BLOCK,Fb_Layer);
}
